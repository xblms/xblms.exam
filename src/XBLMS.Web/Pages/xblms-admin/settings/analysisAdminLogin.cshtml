@page
@{ Layout = "_Layout"; }
<el-card>
    <div slot="header" class="clearfix">
        <el-form v-on:submit.native.prevent :inline="true" :model="form" :size="euiSize">
            <el-form-item label="从">
                <el-date-picker v-model="form.dateFrom"
                                type="date"
                                format="yyyy 年 MM 月 dd 日"
                                value-format="yyyy-MM-dd"
                                placeholder="选择开始日期">
                </el-date-picker>
            </el-form-item>
            <el-form-item label="到">
                <el-date-picker v-model="form.dateTo"
                                type="date"
                                format="yyyy 年 MM 月 dd 日"
                                value-format="yyyy-MM-dd"
                                placeholder="选择结束日期">
                </el-date-picker>
            </el-form-item>

            <el-form-item>
                <el-button :size="euiSize" type="primary" v-on:click="btnSearchClick" icon="el-icon-s-data">统 计</el-button>
            </el-form-item>
        </el-form>
    </div>
    <el-scrollbar class="scrollbar" :style="{ height: ($(window).height()-150) + 'px' }">
        <line-chart :chart-data="chartData"></line-chart>
    </el-scrollbar>

</el-card>
@section Scripts{
<script src="/sitefiles/assets/lib/chartjs/chart-2.7.1.min.js"></script>
<script src="/sitefiles/assets/lib/chartjs/vue-chartjs.min.js"></script>
<script src="/sitefiles/assets/js/admin/settings/analysisAdminLogin.js" type="text/javascript"></script> }