@page
@{ Layout = "_Layout"; }
<el-card>
    <el-form :inline="true" :size="euiSize">
        <el-form-item label="">
            <el-button :size="euiSize" type="primary" plain icon="el-icon-delete" v-on:click="btnCleanClick">清空缓存</el-button>
        </el-form-item>
    </el-form>
    <el-table class="headerless"
              :data="parameters" size="small"
              style="width: 100%" :height="($(window).height() - 115)">
        <el-table-column width="280">
            <template slot-scope="scope">
                <strong>{{scope.row.key}}</strong>
            </template>
        </el-table-column>
        <el-table-column>
            <template slot-scope="scope">
                {{scope.row.value}}
            </template>
        </el-table-column>
        @await Html.PartialAsync("_PartialTableEmpty")
    </el-table>
</el-card>

@section Scripts{ <script src="/sitefiles/assets/js/admin/settings/utilitiesCache.js" type="text/javascript"></script> }