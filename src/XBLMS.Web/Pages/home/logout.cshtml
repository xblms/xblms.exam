@page
@{ Layout = "_LayoutHome"; }
@section Styles{
    <style>
        body {
            padding: 0;
            margin: 0;
            background:#ffffff;
        }

        html {
            padding: 0;
            margin: 0;
            background: #ffffff;
        }
    </style>
}
<div class="bg-cover position-relative" style="background-image:url('/sitefiles/assets/images/bg/bg30.png');">
    <div class="container py-8">
        <span class="p-0 m-0 fw-bolder h1 text-gray-1000 d-flex align-items-center"><i class="el-icon-switch-button me-3 fs-1"></i><span>退出系统</span></span>
    </div>
</div>
<div class="container py-8">
    <el-button icon="el-icon-switch-button" type="warning" plain :size="euiSize" class="py-8 rounded-pill w-100 fs-3" v-on:click="logout">
        安全退出
    </el-button>
    <div class="py-3"></div>
    <el-button icon="el-icon-back" type="success" plain :size="euiSize" class="py-8 rounded-pill w-100 fs-3 bg-white border-0 text-dark" v-on:click="top.$vue.btnTopMenuClick('index')">
        取 消
    </el-button>
</div>
@section Scripts{
<script src="/sitefiles/assets/js/home/logout.js" type="text/javascript"></script> }